func ifelse(int par) int {
  if -par == 10 {
    return 100
  } elif par < 0 { return 2 } 
  elif par <= 2 { return 3
  } elif par > 89 { return 4 } elif par >= 83 {
    return 5
  } 
  else { return par + 7 }
  return 200
}
run int { 
  return ifelse(-10) + ifelse(-5) + ifelse(2) + 
     ifelse(83) + ifelse(100)+ifelse(10)
}
