run arr.arr.str {
  arr.arr.str ai = {{`1`, `2`}, {`3`, `4`, `5`}, {`6`}}
  arr.arr.str ab &= Slice(ai, 1, 3)
  arr.str tmp = {`7`}
  ai[1] &= tmp
  return ab += ai
}
